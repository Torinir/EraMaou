@V_ABLE(ARG)
;ARGのV行為が可能かどうかを判定する関数です
;オトコだとダメ
SIF TALENT:(ARG):122
	RETURN 0
;未熟だとダメ(調教者がサドなら可)
SIF TALENT:(ARG):135
	RETURN 0
;処女だとダメ
SIF TALENT:(ARG):0
	RETURN 0
;貞操帯を付けているとダメ
SIF CFLAG:(ARG):42 == 79 && (CFLAG:(ARG):40 & 64) && FLAG:37
	RETURN 0
;貞操封印だとダメ
SIF TALENT:(ARG):273
	RETURN 0
RETURN 1

;-------------------------------------------------
;0-9 愛撫系コマンド
;-------------------------------------------------
;--------------------------------------------------
;愛撫
;--------------------------------------------------
@COM_ABLE0
;愛撫系フィルタが入ってるとダメ
SIF FLAG:25 & 1
	RETURN 0
;決闘中はダメ
SIF TEQUIP:55
	RETURN 0
RETURN 1
;-------------------------------------------------
;クンニ実行判定
;-------------------------------------------------
@COM_ABLE1
;愛撫系フィルタが入ってるとダメ
SIF FLAG:25 & 1
	RETURN 0
;対象がオトコだとダメ
SIF TALENT:122
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:90
	RETURN 0
;決闘中はダメ
SIF TEQUIP:55
	RETURN 0
;パンツか上着下-ズボンタイプを着てるとダメ
SIF (CFLAG:40 & 17) && FLAG:37
	RETURN 0
;オムツをはいているとダメ
SIF CFLAG:42 == 69 && (CFLAG:40 & 64) && FLAG:37
	RETURN 0
;ズーコの着ぐるみを着てるとダメ
SIF CFLAG:42 == 11 && (CFLAG:40 & 64) && FLAG:37
	RETURN 0
;性器が精液,アナル,尿で汚れており,汚臭敏感で従順3以下のASSIの場合クンニはできない(調教者が汚れ無視ならＯＫ)
IF (STAIN:2 & 4 || STAIN:2 & 8 || STAIN:2 & 32) && ASSIPLAY
	SIF ABL:ASSI:0 <= 3 && TALENT:ASSI:62 && TALENT:ASSI:64 == 0
		RETURN 0
ENDIF
RETURN 1

;-------------------------------------------------
;アナル愛撫実行判定
;-------------------------------------------------
@COM_ABLE2
resetcolor
;愛撫系フィルタが入ってるとダメ
SIF FLAG:25 & 1
	RETURN 0
;決闘中はダメ
SIF TEQUIP:55
	RETURN 0
;獣姦プレイ中はダメ
SIF TEQUIP:89
	RETURN 0
;パンツか上着下-ズボンタイプを着てるとダメ
