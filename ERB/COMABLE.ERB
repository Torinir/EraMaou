;オムツをはいているとダメ
SIF CFLAG:42 == 69 && (CFLAG:40 & 64) && FLAG:37
	RETURN 0
;ズーコの着ぐるみを着てるとダメ
SIF CFLAG:42 == 11 && (CFLAG:40 & 64) && FLAG:37
	RETURN 0
;主人プレイなら自動成功
IF ASSIPLAY == 0
	RETURN 1
;助手プレイの場合
ELSE
	;潤滑不足の場合はダメ
	IF  PALAM:3 < PALAMLV:2
		;助手がサドならOK
		IF TALENT:ASSI:83
			RETURN 1
		;助手が従順4以上かつレズっ気4以上の場合はOK
		ELSEIF ABL:ASSI:10 <= 3 && ABL:ASSI:22 <= 3
			RETURN 1
		ENDIF
		RETURN 0
	ENDIF
ENDIF
RETURN 1

