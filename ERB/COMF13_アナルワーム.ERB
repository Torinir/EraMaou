SIF TALENT:99
	TIMES LOCAL:2 , 0.80
;小柄体型
SIF TALENT:100
	TIMES LOCAL:2 , 2.00
;未熟
SIF TALENT:135
	TIMES SOURCE:6 , 2.00

;A敏感,鈍感をみる
;快A自体のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:106
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

SOURCE:2 += LOCAL:0
SOURCE:13 += LOCAL:1
SOURCE:6 += LOCAL:2

;処女で貞操観念
IF TALENT:0 && TALENT:30
	TIMES SOURCE:13 , 0.80
	TIMES SOURCE:14 , 0.50
ENDIF

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
S = 0
;自慰実行時は経験+1
SIF SELECTCOM == 3
	S = 1
;A感覚が高いほどA経験が入る
IF ABL:3 <= 1
	S += 1
ELSEIF ABL:3 <= 4
	S += 2
ELSEIF ABL:3 <= 7
	S += 3
ELSE
	S += 4
ENDIF
EXP:1 += S
PRINTFORML A経験+{S}
S = 0

;-------------------------------------------------
;その他の処理
;-------------------------------------------------
SIF TEQUIP:90
	T += 1

RETURN 1


