;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;クリキャップ
;道具使用コマンド:調教対象のクリトリスをクリキャップで刺激
;-------------------------------------------------
@COM14

IF TEQUIP:90
	PRINTL 触手クリ責め
	;SAVESTR:22 = 触手クリ責め
ELSE
	PRINTL 電動クリキャップ
	;SAVESTR:22 = 電動クリキャップ
ENDIF
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 10
LOSEBASE:1 += 80

SOURCE:12 = 120
SOURCE:14 = 70

;ABL:Ｃ感覚をみる
IF ABL:0 == 0
	SOURCE:0 = 200
ELSEIF ABL:0 == 1
	SOURCE:0 = 400
ELSEIF ABL:0 == 2
	SOURCE:0 = 900
ELSEIF ABL:0 == 3
	SOURCE:0 = 1600
ELSEIF ABL:0 == 4
	SOURCE:0 = 2400
ELSE
	SOURCE:0 = 3000
ENDIF

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
;レズ経験
IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:40 += 1
ENDIF

;-------------------------------------------------
;クリキャップの着脱
;-------------------------------------------------
TEQUIP:14 = 1 - TEQUIP:14
SIF TEQUIP:90
	T = 0

;-------------------------------------------------
;汚れ(触手)
;-------------------------------------------------
IF TEQUIP:90 && TEQUIP:14
	STAIN:3 |= 2
	STAIN:3 |= 4
ENDIF

RETURN 1

;-------------------------------------------------
;-------------------------------------------------
;クリキャップ装備中
;-------------------------------------------------
@EQUIP_COM14
IF TEQUIP:90
	PRINTL <触手クリ責め中>
ELSE
	PRINTL <クリキャップ装備中>
ENDIF

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 5
LOSEBASE:1 += 20

;ABL:Ｃ感覚をみる
IF ABL:0 == 0
	A = 40
ELSEIF ABL:0 == 1
	A = 120
ELSEIF ABL:0 == 2
	A = 250
ELSEIF ABL:0 == 3
	A = 450
ELSEIF ABL:0 == 4
	A = 600
ELSE
	A = 750
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:10 == 0
	TIMES A , 0.80
ELSEIF ABL:10 == 1
	TIMES A , 0.90
ELSEIF ABL:10 == 2
	TIMES A , 1.00
ELSEIF ABL:10 == 3
	TIMES A , 1.10
ELSEIF ABL:10 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF

SOURCE:0 += A
SOURCE:11 += 50
SOURCE:12 += 50

;-------------------------------------------------
;その他の処理
;-------------------------------------------------
SIF TEQUIP:90
	T += 1

RETURN 1
