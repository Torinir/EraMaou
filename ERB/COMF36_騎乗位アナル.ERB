;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;騎乗位アナル
;奉仕系コマンド:調教対象が調教者のペニスをアナルで刺激
;-------------------------------------------------
@COM36
PRINTL 騎乗位アナル

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:11
	SIF S
		PRINT  + 
	A += ABL:11 * 3
	PRINTS ABLNAME:11
	PRINTV 'LV,ABL:11,'(,ABL:11 * 3,')
	S = 1
ENDIF
;ABL:A感覚
IF ABL:3
	SIF S
		PRINT  + 
	A += ABL:3 * 2
	PRINTS ABLNAME:3
	PRINTV 'LV,ABL:3,'(,ABL:3 * 2,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:16
	SIF S
		PRINT  + 
	A += ABL:16 * 4
	PRINTS ABLNAME:16
	PRINTV 'LV,ABL:16,'(,ABL:16 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:32
	SIF S
		PRINT  + 
	A += ABL:32 * 1
	PRINTS ABLNAME:32
	PRINTV 'LV,ABL:32,'(,ABL:32 * 1,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 3
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 3,')
	S = 1
ENDIF

;PALAM:潤滑(の不足)
IF PALAM:3 < PALAMLV:3
	PRINT  - 
	A -= 5
	PRINT 潤滑不足
	PRINTV '(,5,')
	S = 1
ENDIF
;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 3
	PRINTS PALAMNAME:5
	PRINTV 'LV,L,'(,L * 3,')
	S = 1
ENDIF

;恥じらい
IF TALENT:35
	PRINT  - 
	A -= 2
	PRINTS TALENTNAME:35
	PRINTV '(,2,')
	S = 1
ENDIF
;快感の否定
IF TALENT:71
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:71
	PRINTV '(,5,')
	S = 1
ENDIF
;男嫌い
IF TALENT:82 && TALENT:PLAYER:122
	PRINT  - 
	A -= 12
	PRINTS TALENTNAME:82
	PRINTV '(,12,')
	S = 1
ENDIF
;愛
IF TALENT:85 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:85
	PRINTV '(,5,')
	S = 5
ENDIF

;尻穴狂い
IF TALENT:77
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:77
	PRINTV '(,5,')
	S = 5
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	SIF S
		PRINT  + 
	A += 4
	PRINTS TALENTNAME:121
	PRINTV '(,4,')
	S = 1
ENDIF

;しあわせ草
IF TEQUIP:21
	SIF S
		PRINT  + 
	A += 6
	PRINTS ITEMNAME:26
	PRINTV '(,6,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
;母乳の汚れ
SIF STAIN:PLAYER:2 & 16
	Y += 1
;尿の汚れ
SIF STAIN:PLAYER:4 & 32
	Y += 3

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;騎乗位では汚れの影響が少なめ
Y /= 3

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;汚臭鈍感
	IF TALENT:61
		PRINT 汚れあり,
		PRINTS TALENTNAME:61
	ELSEIF TALENT:62
		PRINT 汚れあり,
		PRINTS TALENTNAME:62
	ELSE
		PRINT 汚れあり
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(24以上で実行)
PRINT  = 
PRINTV A

V = 24
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT Move points:
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0


;-------------------------------------------------
;実行決定
;-------------------------------------------------

;SAVESTR:22 = 騎乗位アナル
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 90
LOSEBASE:1 += 110

SOURCE:12 = 900

;上のほうで計算した汚れデータ
SOURCE:8 = Y*10 + 60

;ABL:奉仕精神をみる
IF ABL:16 == 0
	SOURCE:4 = 200
	SOURCE:5 = 50
	SOURCE:14 = 300
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:16 == 1
	SOURCE:4 = 250
	SOURCE:5 = 200
	SOURCE:14 = 100
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:16 == 2
	SOURCE:4 = 350
	SOURCE:5 = 550
	SOURCE:14 = 30
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:16 == 3
	SOURCE:4 = 450
	SOURCE:5 = 900
	SOURCE:14 = 0
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:16 == 4
	SOURCE:4 = 600
	SOURCE:5 = 1500
	SOURCE:14 = 0
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 750
	SOURCE:5 = 2200
	SOURCE:14 = 0
	TIMES SOURCE:8 , 0.10
ENDIF

;ABL:A感覚をみる
IF ABL:3 == 0
	SOURCE:2 = 10
	SOURCE:3 = 10
	SOURCE:13 += 80
	TIMES SOURCE:4 , 0.50
ELSEIF ABL:3 == 1
	SOURCE:2 = 20
	SOURCE:3 = 50
	SOURCE:13 += 300
	TIMES SOURCE:4 , 0.80
ELSEIF ABL:3 == 2
	SOURCE:2 = 200
	SOURCE:3 = 100
	SOURCE:13 += 700
	TIMES SOURCE:4 , 1.00
ELSEIF ABL:3 == 3
	SOURCE:2 = 450
	SOURCE:3 = 180
	SOURCE:13 += 1500
	TIMES SOURCE:4 , 1.20
ELSEIF ABL:3 == 4
	SOURCE:2 = 850
	SOURCE:3 = 300
	SOURCE:13 += 2600
	TIMES SOURCE:4 , 1.50
ELSE
	SOURCE:2 = 1300
	SOURCE:3 = 450
	SOURCE:13 += 4000
	TIMES SOURCE:4 , 2.00
ENDIF

;EXP:A経験をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.10
	SOURCE:6 = 18000
	LOSEBASE:0 += 70
	LOSEBASE:1 += 80
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 0.30
	SOURCE:6 = 10000
	LOSEBASE:0 += 60
	LOSEBASE:1 += 70
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 0.50
	SOURCE:6 = 4500
	LOSEBASE:0 += 45
	LOSEBASE:1 += 55
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.00
	SOURCE:6 = 1500
	LOSEBASE:0 += 25
	LOSEBASE:1 += 35
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:6 = 700
	LOSEBASE:0 += 10
	LOSEBASE:1 += 15
ELSE
	TIMES SOURCE:2 , 1.60
	SOURCE:6 = 300
	LOSEBASE:0 += 0
	LOSEBASE:1 += 0
ENDIF


;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:6 += 10000
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:6 += 3600
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:6 += 1200
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:6 += 200
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:2 , 1.80
	SOURCE:6 += 100
ENDIF

;大柄
SIF TALENT:99
	TIMES SOURCE:6 , 0.80
;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00
;未熟
SIF TALENT:135
	TIMES SOURCE:6 , 2.00

;貞操観念
IF TALENT:30
	IF EXP:1 == 0
		TIMES SOURCE:3 , 1.20
		TIMES SOURCE:16 , 1.10
		SOURCE:15 = 500
	ELSE
		TIMES SOURCE:3 , 1.20
		TIMES SOURCE:16 , 1.10
		SOURCE:15 = 0
	ENDIF
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:2 , 0.60
	TIMES SOURCE:13 , 0.60
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	TIMES SOURCE:13 , 0.80
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:13 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:2 , 1.20
	TIMES SOURCE:13 , 1.20
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:2 , 1.40
	TIMES SOURCE:13 , 1.40
ENDIF


;ABL:従順をみる
IF ABL:10 == 0
	TIMES SOURCE:2 , 0.50
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
	SOURCE:13 += 700
ELSEIF ABL:10 == 1
	TIMES SOURCE:2 , 0.80
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
	SOURCE:13 += 500
ELSEIF ABL:10 == 2
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
	SOURCE:13 += 300
ELSEIF ABL:10 == 3
	TIMES SOURCE:2 , 1.30
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
	SOURCE:13 += 100
ELSEIF ABL:10 == 4
	TIMES SOURCE:2 , 1.60
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
	SOURCE:13 += 30
ELSE
	TIMES SOURCE:2 , 2.00
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
	SOURCE:13 += 0
ENDIF

;ABL:技巧をみる
IF ABL:12 == 0
	TIMES SOURCE:2 , 0.50
	TIMES SOURCE:3 , 0.60
ELSEIF ABL:12 == 1
	TIMES SOURCE:2 , 0.80
	TIMES SOURCE:3 , 0.80
ELSEIF ABL:12 == 2
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:2 , 1.30
	TIMES SOURCE:3 , 1.20
ELSEIF ABL:12 == 4
	TIMES SOURCE:2 , 1.60
	TIMES SOURCE:3 , 1.40
ELSE
	TIMES SOURCE:2 , 2.00
	TIMES SOURCE:3 , 1.60
ENDIF

;調教者の射精チェック
CALL COM_EJAC_PLAYER_ANALSEX

;射精している
IF E
	TIMES SOURCE:4 , 3.00
	TIMES SOURCE:5 , 5.00
	TIMES SOURCE:13 , 1.50

	;ABL:精液中毒をみる
	IF ABL:32 == 0
		SOURCE:7 = 0
	ELSEIF ABL:32 == 1
		SOURCE:7 = 300
	ELSEIF ABL:32 == 2
		SOURCE:7 = 1000
	ELSEIF ABL:32 == 3
		SOURCE:7 = 1500
	ELSEIF ABL:32 == 4
		SOURCE:7 = 2200
	ELSE
		SOURCE:7 = 3000
	ENDIF
ENDIF

;アナルセックス後の処理
CALL COM_AFTER_ANAL_SEX

TFLAG:100 = 1

IF EXP:1 <= 1
	;屈服刻印３に相当
	TFLAG:200 = 3
ELSE
	;屈服刻印2に相当
	TFLAG:200 = 2
ENDIF

;調教者がふたなり
SIF TALENT:PLAYER:121
	SOURCE:13 /= 2

RETURN 1
