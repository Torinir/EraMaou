;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;足コキする
;SM系コマンド:調教者が調教対象のペニスを足で刺激
;-------------------------------------------------
@COM48

PRINTL 足コキする
;SAVESTR:22 = 足コキする
CALL TRAIN_MESSAGE_B

LOSEBASE:0 += 10
LOSEBASE:1 += 60

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
SOURCE:12 = 150
SOURCE:14 = 400

;ABL:Ｃ感覚をみる
IF ABL:0 == 0
	SOURCE:0 = 30
ELSEIF ABL:0 == 1
	SOURCE:0 = 100
ELSEIF ABL:0 == 2
	SOURCE:0 = 200
ELSEIF ABL:0 == 3
	SOURCE:0 = 500
ELSEIF ABL:0 == 4
	SOURCE:0 = 1000
ELSE
	SOURCE:0 = 1500
ENDIF

;ABL:マゾっ気をみる
IF ABL:21 == 0
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:14 , 1.00
ELSEIF ABL:21 == 1
	TIMES SOURCE:0 , 1.20
	TIMES SOURCE:14 , 0.80
ELSEIF ABL:21 == 2
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:14 , 0.60
ELSEIF ABL:21 == 3
	TIMES SOURCE:0 , 1.80
	TIMES SOURCE:14 , 0.40
ELSEIF ABL:21 == 4
	TIMES SOURCE:0 , 2.20
	TIMES SOURCE:14 , 0.20
ELSE
	TIMES SOURCE:0 , 3.00
	TIMES SOURCE:14 , 0.00
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
;被虐快楽経験
IF TALENT:88 == 1 || (ABL:11 >= 3 && ABL:21 >= 3)
	PRINTFORML %EXPNAME:30%+3
	EXP:30 += 3
ELSEIF ABL:11 >= 3 && ABL:21 >= 1
	PRINTFORML %EXPNAME:30%+2
	EXP:30 += 2
ELSEIF ABL:11 >= 3 || ABL:21 >= 1
	PRINTFORML %EXPNAME:30%+1
	EXP:30 += 1
ENDIF

;レズ-ホモ経験
IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +3
	EXP:40 += 3
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +3
	EXP:41 += 3
ENDIF

CALL EVENT_SEITSU_ASIKOKI

;愛情経験
IF TALENT:122
	E = 2
ELSE
	E = 1
ENDIF
IF CFLAG:2 >= 1000 && ASSIPLAY == 0
	PRINTFORML %EXPNAME:23%+{E}
	EXP:23 += E
ENDIF
E = 0

RETURN 1
;
;-------------------------------------------------
;-------------------------------------------------
;精通(足コキ)
;-------------------------------------------------
@EVENT_SEITSU_ASIKOKI
A = NO:PLAYER
;[オトコ]か[ふたなり]であり[未熟]
SIF (TALENT:121 == 0 && TALENT:122 == 0) || TALENT:135 == 0
	RETURN 0
;更に［Ｃ感度］5以上で触手調教や獣姦でなく
SIF ABL:0 <= 4 || TEQUIP:90 || TEQUIP:89
	RETURN 0
;"調教対象"から"調教者"への関係が150以上
SIF RELATION:TARGET:A < 150
	RETURN 0
PRINTFORML %SAVESTR:PLAYER%に足でペニスを踏まれる内に,%SAVESTR:TARGET%は精通を迎えた...
TALENT:135 = 0

RETURN 1
;
