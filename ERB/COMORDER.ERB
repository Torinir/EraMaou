;相性チェック
;-------------------------------------------------
R = NO:PLAYER
IF RELATION:R > 0 && RELATION:R < 30
	PRINT  - 
	A -= 10
	PRINT Worst affinity
	PRINTV '(,10,')
	S = 1
ELSEIF RELATION:R > 0 && RELATION:R < 70
	PRINT  - 
	A -= 6
	PRINT Bad affinity
	PRINTV '(,6,')
	S = 1
ELSEIF RELATION:R > 0 && RELATION:R < 100
	PRINT  - 
	A -= 3
	PRINT Normal affinity
	PRINTV '(,3,')
	S = 1
