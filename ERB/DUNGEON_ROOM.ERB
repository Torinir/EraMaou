	PRINTL  
ENDIF

;判定
MENU = 0

;MENU = 1 ショタコン
;MENU = 2 ロリコン
;MENU = 3 男淫魔
;MENU = 4 女淫魔

;カルマが低い非処女の場合
SIF CFLAG:A:151 < -20 && TALENT:A:0 == 0
	MENU = 3
;カルマが低いレズっ気の場合
SIF CFLAG:A:151 < 0 && ABL:A:22 > 0
	MENU = 4
;カルマが低いふたなりの場合
SIF CFLAG:A:151 < 30 && TALENT:A:121
	MENU = 4
;カルマが低いオトコの場合
SIF CFLAG:A:151 < 10 && TALENT:A:122
	MENU = 4
;ショタコン
SIF TALENT:A:143
	MENU = 1
;ロリコン
SIF TALENT:A:142
	MENU = 2

IF MENU == 0
	IF FLAG:5 & 32
		printformw %SAVESTR:A%は嫌悪の目で街を通り過ぎた...
	ENDIF
	RETURN 0
ENDIF

COST = (CFLAG:A:9 * 8)+150

;オプション
SIF ARG:0 & 1
	TIMES COST, 1.1
SIF ARG:0 & 2
	TIMES COST, 1.1

IF CFLAG:A:580 < COST
	IF FLAG:5 & 32
		printformw %SAVESTR:A%は手持ちの金がないようだ...
	ENDIF
	RETURN 0
ENDIF

MONEY += COST
CFLAG:A:580 -= COST
CALL KARMA, A, -1

IF FLAG:5 & 32
	printform %SAVESTR:A%は娼館街で
	IF MENU == 1
		PRINT 少年奴隷
	ELSEIF MENU == 2
		PRINT 少女奴隷
	ELSEIF MENU == 3
		PRINT 男淫魔
	ELSEIF MENU == 4
		PRINT 女淫魔
	ENDIF
	printform 相手に
	IF MENU == 1 || MENU == 2
		PRINT 地上では楽しめない背徳を楽しんだようだ......
	ELSE
		PRINT お楽しみしていったようだ...
	ENDIF
	printl (カルマが1減少した)
	printformw 現金収入+{COST}
ENDIF

RETURN 0


