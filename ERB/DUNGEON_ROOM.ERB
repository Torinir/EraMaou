	SIF ARG:0 == 0
		print :なし
	SIF ARG:0 & 1
		print :巡回ゴーレム
	SIF ARG:0 & 2
		print :陳列棚
	PRINTL  
ENDIF

;石像と剥製ないとダメ
SIF FLAG:84 <= 0
	RETURN 0

MDMG = FLAG:84 * 5
DMG = 0

SIF ARG:0 & 1
	DMG += FLAG:84 * 2

SIF MDMG > (MAXBASE:A:1 / 4)
	MDMG = MAXBASE:A:1 / 4

IF FLAG:5 & 32
	printforml %SAVESTR:A%は勇者のなれの果てである調度品を見て身も心も震えている.........(気力-{MDMG})
	SIF DMG > 0
		printforml 犠牲者から作られたゴーレムが襲いかかる!({DMG}のダメージ!)
	PRINTW 
ENDIF

IF RAND:4 == 0 && ARG:0 & 2
	;陳列棚
	SIF FLAG:5 & 32
		printformw 陳列棚がじゃまで遠隔攻撃ができそうにない......(先制不可!)
	IF CFLAG:A:503 & 32
		SIF FLAG:5 & 32
			PRINTL すでに先制はできない
	ELSE
		CFLAG:A:503 += 32
	ENDIF
ENDIF

BASE:A:1 -= MDMG
SIF DMG > 0
	BASE:A:0 -= DMG

;最低1は残るタイプ
SIF BASE:A:0 < 1
	BASE:A:0 = 1

RETURN 0

