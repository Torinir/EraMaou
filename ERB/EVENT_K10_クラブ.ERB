	;悲しんで
	PRINTFORM なんだよ.......
ELSEIF ARG:0 == 4
	;恥ずかしそうに
	PRINTFORM なの.......
ELSEIF ARG:0 == 5
	;情けなさそうに
	PRINTFORM だわ.......
ELSE
	;デフォルト
	;ARG:0==0を含む
	IF RAND:3 == 0
		PRINTFORM なの.
	ELSEIF RAND:2 == 0
		PRINTFORM だから.
	ELSE
		PRINTFORM だわ.
	ENDIF
ENDIF





