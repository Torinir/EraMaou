			IF (TALENT:愛 || TALENT:淫乱)
				SELECTCASE RAND:3
				CASE 2
					LOCALS = %LOCALS%!
				CASE 1
					LOCALS = %LOCALS%%UNICODE(0x2661) *1%
				CASE 0
					LOCALS = %LOCALS%♪
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
				CASE 1
					LOCALS = %LOCALS%!
				CASE 0
					LOCALS = %LOCALS%!!
				ENDSELECT
			ENDIF
		ENDIF
	ENDSELECT
NEXT
RETURNF LOCALS

