	;オムツの場合は洗濯速度は下着並
	SIF CFLAG:42 == 69
		CFLAG:47 = 2
	TFLAG:45 -= 16
	S = 1
	SIF CFLAG:40 & 64
		CFLAG:40 -= 64
ENDIF
;上着下
IF CFLAG:41 && (TFLAG:45 & 8)
	PRINTFORM (%SAVESTR:TARGET%が着ていた
	CALL PRINT_CLOTHTYPE_MAIN2
	IF CFLAG:41 >= 1 && CFLAG:41 <= 100
		PRINT のスカ-ト
	ELSEIF CFLAG:41 <= 200
		PRINT 下
	ENDIF
	PRINTW は廃棄処分にされました)
	IF CFLAG:41 >= 201
		CFLAG:41 = 0
		SIF CFLAG:40 & 4
			CFLAG:40 -= 4
		SIF CFLAG:40 & 8
			CFLAG:40 -= 8
		SIF CFLAG:40 & 16
			CFLAG:40 -= 16
	ELSE
		CFLAG:46 = -2
		SIF CFLAG:40 & 8
			CFLAG:40 -= 8
		SIF CFLAG:40 & 16
			CFLAG:40 -= 16
	ENDIF
	TFLAG:45 -= 8
	S = 1
ELSEIF CFLAG:41 && (TFLAG:45 & 4) && CFLAG:46 == 0
	PRINTFORM (%SAVESTR:TARGET%が着ていた
	CALL PRINT_CLOTHTYPE_MAIN2
	IF CFLAG:41 >= 1 && CFLAG:41 <= 100
		PRINT のスカ-ト
	ELSEIF CFLAG:41 <= 200
		PRINT 下
	ENDIF
	PRINTW を洗濯に出しました)
	IF CFLAG:41 >= 201
		CFLAG:45 = 3
		CFLAG:46 = 3
		SIF CFLAG:40 & 4
			CFLAG:40 -= 4
		SIF CFLAG:40 & 8
			CFLAG:40 -= 8
		SIF CFLAG:40 & 16
			CFLAG:40 -= 16
	ELSE
		CFLAG:46 = 3
		SIF CFLAG:40 & 8
			CFLAG:40 -= 8
		SIF CFLAG:40 & 16
			CFLAG:40 -= 16
	ENDIF
	TFLAG:45 -= 4
	S = 1
ENDIF
;パンツ
IF (TFLAG:45 & 2)
	PRINTFORMW (%SAVESTR:TARGET%の下着は廃棄処分にされました)
	CFLAG:43 = -2
	SIF CFLAG:40 & 1
		CFLAG:40 -= 1
	TFLAG:45 -= 2
	S = 1
