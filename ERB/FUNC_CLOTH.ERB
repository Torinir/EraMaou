SIF CFLAG:45 != 0
	CFLAG:40 -= (CFLAG:40 & 4)
SIF CFLAG:46 != 0
	CFLAG:40 -= (CFLAG:40 & 8)
SIF CFLAG:46 != 0
	CFLAG:40 -= (CFLAG:40 & 16)
SIF CFLAG:47 != 0
	CFLAG:40 -= (CFLAG:40 & 64)

;-------------------------------------------------
;調教後の衣類の処理
;-------------------------------------------------
@AFTERTRAIN_CLOTH
;汚れた衣類の始末
