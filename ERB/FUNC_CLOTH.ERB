				PRINT 乳房むきだしに
			ELSE
				PRINT 上半身裸に
			ENDIF
		ENDIF
		CALL PRINT_CLOTHTYPE_MAIN2
		IF CFLAG:41 >= 1 && CFLAG:41 <= 100
			PRINT のスカ-ト姿
		ELSE
			PRINT 下だけの姿
		ENDIF
	ELSE
		CALL PRINT_CLOTHTYPE_MAIN2
		SIF (CFLAG:40 & 64) == 0
			PRINT 姿
	ENDIF
ELSEIF (CFLAG:40 & 1) && (CFLAG:40 & 2)
