	PRINT スライムに包まれた姿
	RETURN 1
ENDIF

;基本コスチューム処理
CALL PRINT_CLOTHTYPE_MAIN

;特別コスチューム処理
IF CFLAG:42
	PRINT に
	CALL PRINT_CLOTHTYPE_SPECIAL
	PRINT 姿
ENDIF

RETURN 1

;基本コスチューム
