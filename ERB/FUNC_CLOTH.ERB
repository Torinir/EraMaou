	PRINTFORMW (%SAVESTR:TARGET%の下着を洗濯に出しました)
	CFLAG:43 = 2
	SIF CFLAG:40 & 1
		CFLAG:40 -= 1
	TFLAG:45 -= 1
	S = 1
ENDIF

;上下ともダメになった衣装は削除
IF CFLAG:41
	SIF CFLAG:45 < 0 && CFLAG:46 < 0
		CFLAG:41 = 0
	;ふんどし用処理
	SIF CFLAG:41 == 192 && CFLAG:46 < 0
		CFLAG:41 = 0
ENDIF
;ダメになった特別コスは削除
IF CFLAG:42
	SIF CFLAG:47 < 0
		CFLAG:42 = 0
ENDIF

RETURN 1

;-------------------------------------------------
;衣類の再着衣
;-------------------------------------------------
@RE_CLOTHED
;従順+露出癖3以上なら,主人に脱がされた状態を維持
;それ以外なら着られる衣類は全部身に着ける
IF ABL:10 + ABL:17 < 3
	C = CFLAG:40
	CALL WEARING_CLOTH_ABLE
	IF CFLAG:40 > C
		PRINTFORML (%SAVESTR:TARGET%は,脱がされていた衣装を再び身に付けました)
		WAIT
	ENDIF
ENDIF

RETURN 1

;-------------------------------------------------
;汚れた衣類の洗濯
;-------------------------------------------------
@WASHING_CLOTH
S = 0
REPEAT CHARANUM
	TARGET = COUNT
	;全身衣装の場合,洗濯の進行状況は下半身部分にあわせる
	SIF CFLAG:41 >= 200 && CFLAG:45 > 0 && CFLAG:46 > 0
		CFLAG:45 = CFLAG:46
	;下着の洗濯
	IF CFLAG:43 > 0
		CFLAG:43 -= 1
		IF CFLAG:43 == 0
			PRINTFORML (%SAVESTR:TARGET%の下着が洗濯から戻ってきました)
			S = 1
		ENDIF
	ENDIF
	;通常コスの洗濯
	IF CFLAG:46 > 0
		CFLAG:46 -= 1
		IF CFLAG:46 == 0 && CFLAG:41
			PRINTFORM (%SAVESTR:TARGET%の
			CALL PRINT_CLOTHTYPE_MAIN2
			PRINTFORML が洗濯から戻ってきました)
			S = 1
			SIF CFLAG:41 >= 200 && CFLAG:45 >= 0
				CFLAG:45 = 0
		ENDIF
	ENDIF
	;特別コスの洗濯
	IF CFLAG:47 > 0
		CFLAG:47 -= 1
		IF CFLAG:47 == 0 && CFLAG:42
			PRINTFORM (%SAVESTR:TARGET%の
			CALL PRINT_CLOTHTYPE_SPECIAL
			PRINTFORML が洗濯から戻ってきました)
			S = 1
		ENDIF
	ENDIF

	;パンツのはき古し進行もここで
	SIF (CFLAG:40 & 1)
		CFLAG:48 += 1

REND
IF S
	WAIT
	DRAWLINE
ENDIF

RETURN 1

