		TFLAG:45 -= 16
		S = 1
		SIF CFLAG:40 & 64
			CFLAG:40 -= 64
	ELSE
		GOTO INPUT_LOOP_01
	ENDIF
ELSEIF CFLAG:42 && (TFLAG:45 & 16) && CFLAG:47 == 0
	PRINTFORM (%SAVESTR:TARGET%の
	CALL PRINT_CLOTHTYPE_SPECIAL
