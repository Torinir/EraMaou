	FOR LOCAL:1,0,4
		IF RAND:2 == 0
			;娘
			LOCAL += 100
		ELSE
			;息子
			LOCAL += 1000
		ENDIF
		;ループ抜けの位置が違うので、一人は確定する
		SIF RAND:2 == 0
			BREAK
	NEXT
ELSEIF LOCAL >= 10 && TALENT:0 == 0
