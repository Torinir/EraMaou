		CFLAG:(ARG:0):102 = 4
		CFLAG:(ARG:0):105 = LOVE_EXP:4
	CASE 81
		;犬
		CFLAG:(ARG:0):102 = 5
		CFLAG:(ARG:0):106 = LOVE_EXP:4
	CASE 82 TO 100
		;その他の獣
		CFLAG:(ARG:0):102 = 6
		CFLAG:(ARG:0):107 = LOVE_EXP:4
	ENDSELECT
ENDIF

IF MARRIAGE != 902 && NO_KARMA == 0
	PRINTW (カルマが減少しました:-1)
	CALL KARMA, ARG:0, -1
ELSE
	WAIT
ENDIF

CFLAG:(ARG:0):607 += 1

RETURN 1

