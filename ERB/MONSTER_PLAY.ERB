@MONSTER_PLAY
;-----------------------------------
$INPUT_LOOP

CALL MONSTERPLAY_LIST

SIF ITEM:22 >= 1
	PRINT [900] 野良犬 
PRINTL [999] もどる

INPUT

IF RESULT == 999
	RETURN 0
ELSEIF ITEM:RESULT <= 0 && RESULT != 900
	GOTO INPUT_LOOP
ELSEIF RESULT == 900 && ITEM:22 <= 0
	GOTO INPUT_LOOP
ENDIF

LOCAL:0 = RESULT
IF LOCAL:0 != 900
	CALL MONSTER_DATA, LOCAL:0, 5
	LOCAL:1 = E:507
ENDIF

A = 0
Y = RAND:5 + 3

IF LOCAL:0 == 900
	CALL MONSTER_PLAY_DOG
ELSEIF LOCAL:1 == 1
	CALL ORC_MONSTER_PLAY
ELSEIF LOCAL:1 == 2
	CALL SLIME_MONSTER_PLAY
ELSEIF LOCAL:1 == 3
	CALL INSECT_MONSTER_PLAY
ELSEIF LOCAL:1 == 4
	CALL IVY_MONSTER_PLAY
ELSEIF LOCAL:1 == 5
	CALL SYOKUSYU_MONSTER_PLAY
ELSEIF LOCAL:1 == 6
	CALL FAILY_MONSTER_PLAY
ELSEIF LOCAL:1 == 7
	CALL GIANT_MONSTER_PLAY
ELSEIF LOCAL:1 == 8
	CALL MAN_MONSTER_PLAY
ELSEIF LOCAL:1 == 9
	CALL GIRL_MONSTER_PLAY
ELSEIF LOCAL:1 == 10
	CALL BEAST_MONSTER_PLAY
ELSEIF LOCAL:1 == 11
	CALL BRAIN_MONSTER_PLAY
ELSEIF LOCAL:1 == 12
	CALL HORSE_MONSTER_PLAY
ENDIF

IF TALENT:A:0 == 1 && EXP:A:0 > 0
	PRINTW [処女喪失]
	TALENT:A:0 = 0
ENDIF



BEGIN TURNEND
RETURN 1

