@EVENT_VIDEO_DAY
#DIM VIEW
#DIM FAV
;-----------------------------------

SIF TARGET < 0 || TARGET > CHARANUM
	RETURN 0

;非公開ははじく
SIF CFLAG:497 == 0
	RETURN 0

VIEW = 0
FAV = 0

;評価ボーナス
VIEW += CFLAG:493 / 10

;マニアックさによるデメリット
IF CFLAG:494 > 0 && CFLAG:494 < 100
	VIEW *= 100 - CFLAG:494
	VIEW /= 100
	VIEW += 1
ENDIF

;ふぁぼは閲覧数の1%程度
FAV = (VIEW / 100) + RAND:2
;マニアックボーナス
FAV *= (CFLAG:494 / 2) + 1
;閲覧数以上のふぁぼは得られない
SIF FAV > VIEW
	FAV = VIEW

;加算
CFLAG:495 += VIEW
CFLAG:496 += FAV

IF CSTR:6 != ""
	PRINTL  
	PRINTFORML 水晶に納められた"%CSTR:6%"を裏街道で売りさばいた......
	IF FAV > 0
		PRINTFORMW {VIEW}ポイントのお金を稼ぎ,{FAV}通のファンレタ-が届いた
	ELSE
		PRINTFORMW {VIEW}ポイントのお金を稼いだ
	ENDIF
	
	MONEY += VIEW
	
	PRINTL  
ENDIF

RETURN 0

