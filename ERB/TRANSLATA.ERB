@MAOU_HIS_HER(CAPITALIZE)
#DIM CAPITALIZE=0
#FUNCTIONS
IF TALENT:0:121 == 1
	;; Futanari
	IF CAPITALIZE == 1
		RETURNF "Their"
	ENDIF
	RETURNF "their"

ELSEIF TALENT:0:1 == 1
	;; Male, Young Boy
	IF CAPITALIZE == 1
		RETURNF "His"
	ENDIF
	RETURNF "his"
ELSE
	;; Female
	IF CAPITALIZE == 1
		RETURNF "Her"
	ENDIF
	RETURNF "her"
ENDIF

