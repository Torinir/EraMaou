;SHOW_USERCOM
;FLAG:25 & 1 愛撫系フィルタ
;FLAG:25 & 2 器具系フィルタ
;FLAG:25 & 4 A性交系フィルタ
;FLAG:25 & 8 V性交系フィルタ 
;FLAG:25 & 16 ＳＭ系フィルタ
@SHOW_USERCOM
resetcolor
PRINTC Display Abilities[100]
PRINTC Display Filth[101]
IF ASSI > 0
	PRINTC Switch To Assistant[102]
	PRINTL 
ENDIF
PRINTC Condom Settings[103]
printl
IF FLAG:25 & 1
	SETCOLOR 100,100,100
	PRINTC Caress Filter[104]
	resetcolor
ELSE
	PRINTC Caress Filter[104]
ENDIF
IF FLAG:25 & 2
	SETCOLOR 100,100,100
	PRINTC Tools Filter[105]
	resetcolor
ELSE
	setcolor 100,149,237
	PRINTC Tools Filter[105]
	resetcolor
ENDIF
IF FLAG:25 & 4
	SETCOLOR 100,100,100
	PRINTC V Filter[106]
	resetcolor
ELSE
	setcolor 255,165,0
	PRINTC V Filter[106]
	resetcolor
ENDIF
PRINTL 
IF FLAG:25 & 8
	SETCOLOR 100,100,100
	PRINTC A Filter[107]
	resetcolor
ELSE
	setcolor 219,112,147
	PRINTC A Filter[107]
	resetcolor
ENDIF
IF FLAG:25 & 16
	SETCOLOR 100,100,100
	PRINTC SM Filter[108]
	resetcolor
ELSE
	setcolor 255,99,71
	PRINTC SM Filter[108]
	resetcolor
ENDIF
PRINTC End Training[200]
printc
printl
IF PREVCOM > -1
	CALL P_C
	PRINT <Previous Command:
	SETCOLOR 0xDDA0DD
	PRINTFORM %TSTR:90%
	RESETCOLOR
	PRINTL >
ENDIF

@USERCOM
IF RESULT == 100
	CALL SHOW_CHARA_INFO(TARGET)
	RETURN 1
ELSEIF RESULT == 101
	CALL STAIN_INFO
	RETURN 1
ELSEIF RESULT == 102 && ASSI > 0
	IF ASSIPLAY
		ASSIPLAY = 0
		PLAYER = MASTER
	ELSE
		ASSIPLAY = 1
		PLAYER = ASSI
	ENDIF
	RETURN 1
ELSEIF RESULT == 103
	CALL CONDOM_SETTINGS
	RETURN 1
ELSEIF RESULT == 104
	IF FLAG:25 & 1
		FLAG:25 &= 30
	ELSE
		FLAG:25 |= 1
	ENDIF
ELSEIF RESULT == 105
	IF FLAG:25 & 2
		FLAG:25 &= 29
	ELSE
		FLAG:25 |= 2
	ENDIF
ELSEIF RESULT == 106
	IF FLAG:25 & 4
		FLAG:25 &= 27
	ELSE
		FLAG:25 |= 4
	ENDIF
ELSEIF RESULT == 107
	IF FLAG:25 & 8
		FLAG:25 &= 23
	ELSE
		FLAG:25 |= 8
	ENDIF
ELSEIF RESULT == 108
	IF FLAG:25 & 16
		FLAG:25 &= 15
	ELSE
		FLAG:25 |= 16
	ENDIF

ELSEIF RESULT == 200
	BEGIN AFTERTRAIN
	RETURN 1
ENDIF
RETURN 0
